#include <math.h>
#include <stdio.h>
int main(){
    long int t; 
    scanf("%ld",&t);
    for(long int a0 = 0; a0 < t; a0++){
        long int n; 
        scanf("%ld",&n);
        long int sum1 =  pow(n*(n+1)/2,2);
        long int sum2 = (n*(n+1)*(2*n+1)/6);
        long int res = sum1-sum2;
        printf("%ld\n",res);
    }
    return 0;
}
